def sum():
    n = int(input("Insert area of "))
    a = []
    b = []
    i = 0
    for _ in range(n):
        i = i + 1
        x = int(input("Insert numbers: "))
        a.append(x)
        if (i == 1):
            b.append(x)
        else:
            y = max(b)
            b.append(x + y)

        print("Original array:", a)
        print("New array:", b)

sum()
